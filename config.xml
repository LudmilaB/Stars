<?xml version = "1.0" encoding = "UTF-8"?>

<widget xmlns = "http://www.w3.org/ns/widgets" 
   xmlns:gap = "http://phonegap.com/ns/1.0" id = "com.shapesmania.fillstars" version = "1.03">
   
   <name>Fill Stars</name>
   
   <description>
      Fill maximum number of stars
   </description>
   
   <author href = "http://shapesmania.com/" email = "wheretoplaychess@gmail.com">
      Shapes Mania
   </author>
   
   <preference name = "permissions" value = "none"/>
   
   <icon src = "res/android/icon_ldpi.png" 
      gap:platform = "android" gap:qualifier = "ldpi" />
		
   <icon src = "res/android/icon_mdpi.png" 
      gap:platform = "android" gap:qualifier = "mdpi" />
		
   <icon src = "res/android/icon_hdpi.png" 
      gap:platform = "android" gap:qualifier = "hdpi" />
		
   <icon src = "res/android/icon_xhdpi.png" 
      gap:platform = "android" gap:qualifier = "xhdpi" />
		
   <icon src = "res/icon/android/icon_xxhdpi.png" 
      gap:platform = "android" gap:qualifier = "xxhdpi" />
   
   <icon src = "res/ios/Icon-72.png" gap:platform = "ios" gap:qualifier = ""/>
   <icon src = "res/ios/icon-57.png" gap:platform = "ios" width = "57" height = "57" />
   <icon src = "res/ios/icon-72.png" gap:platform = "ios" width = "72" height = "72" />
   <icon src = "res/ios/icon-57-2x.png" gap:platform = "ios" width = "114" height = "114" />
   <icon src = "res/ios/icon-72-2x.png" gap:platform = "ios" width = "144" height = "144" />
   
</widget>